{
  "name": "next-amplify",
  "private": true,
  "description": "This is example of Amplify Next.js App",
  "author": "Przemek Lewtak",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prebuild": "rimraf dist",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "test:cov": "jest --coverage",
    "test-e2e:cov": "jest --coverage --config ./jest-e2e.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types",
    "prettier": "prettier {*,src/*,src/**/*} --write --ignore-unknown",
    "prettier-watch": "onchange ',src/**/*.tsx' -- prettier --write {{changed}}",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "@mui/styles": "^5.4.1",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.11.0",
    "aws-amplify": "^4.3.14",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "eslint-plugin-import": "^2.25.4",
    "lodash": "^4.17.21",
    "next": "12.0.10",
    "next-images": "^1.8.4",
    "next-redux-wrapper": "^7.0.5",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.26.1",
    "react-redux": "^7.2.6",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.3",
    "swr": "^1.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@babel/types": "^7.17.0",
    "@next/eslint-plugin-next": "^12.0.10",
    "@testing-library/react": "^12.1.2",
    "@types/node": "17.0.16",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.39",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.22",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^27.5.0",
    "next-router-mock": "^0.6.5",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.5"
  }
}
